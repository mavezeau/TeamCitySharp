<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	<Product>MvsTelecom.Teamcity.Api</Product>
    <Company>MVS Telecom</Company>
    <Authors>MVS Telecom</Authors>
    <Version>0.0.0.3</Version>
    <PackageProjectUrl>https://github.com/MVS-Telecom/mvstelecom-teamcity-api</PackageProjectUrl>
    <PackageIcon>icon.png</PackageIcon>
    <Description>C# API client for TeamCity</Description>
    <PackageId>MvsTelecom.Teamcity.Api</PackageId>
  </PropertyGroup>
  
  
  <PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard2.0'">
    <DefineConstants>NETCORE;NETSTANDARD;NETSTANDARD2_0</DefineConstants>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="11.0.2" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\..\icon.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>
  
</Project>